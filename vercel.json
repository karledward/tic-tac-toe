{
  "buildCommand": "pnpm build:client",
  "outputDirectory": "dist/public",
  "rewrites": [
    {
      "source": "/api/trpc/:path*",
      "destination": "/api/trpc/:path*"
    },
    {
      "source": "/api/oauth/:path*",
      "destination": "/api/oauth/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}

